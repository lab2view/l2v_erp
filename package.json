{
  "name": "erp-app",
  "version": "0.3.9",
  "author": {
    "name": "Lab2view",
    "email": "contact@lab2view.com",
    "url": "https://lab2view.com"
  },
  "description": "L2V ERP for Enterprises",
  "homepage": "https://kitbussiness.com",
  "build": {
    "appId": "com.kitbussiness",
    "extends": null,
    "publish": {
      "provider": "github",
      "repo": "l2v_erp",
      "owner": "lab2view",
      "host": "github.com",
      "protocol": "https",
      "private": false
    }
  },
  "engines": {
    "node": ">=v16.13",
    "npm": ">=8.1"
  },
  "main": "packages/main/dist/index.cjs",
  "scripts": {
    "build": "node scripts/build.js",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config .electron-builder.config.js --dir --config.asar=false",
    "asar": "electron-builder build --config .electron-builder.config.js --dir --config.asar=true",
    "pretest": "npm run build",
    "test": "node tests/app.spec.js",
    "watch": "node scripts/watch.js",
    "lint": "eslint . --ext js,vue --ignore-path .gitignore --fix",
    "format": "prettier . --write"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "2.0.1",
    "cross-env": "7.0.3",
    "electron": "^22.0.0",
    "electron-builder": "^22.10.3",
    "electron-builder-squirrel-windows": "^24.0.0-alpha.8",
    "electron-devtools-installer": "3.2.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^8.2.0",
    "nano-staged": "0.5.0",
    "playwright": "1.17.1",
    "prettier": "2.5.1",
    "simple-git-hooks": "2.7.0",
    "vite": "2.7.2",
    "vite-plugin-eslint": "^1.3.0"
  },
  "dependencies": {
    "@imengyu/vue3-context-menu": "^1.0.9",
    "@vuepic/vue-datepicker": "^3.5.2",
    "axios": "^0.24.0",
    "babel-eslint": "^10.1.0",
    "electron-updater": "4.6.5",
    "jquery": "^3.5.1",
    "jsprintmanager": "^5.0.0",
    "laravel-echo": "^1.11.4",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "register-service-worker": "^1.7.2",
    "socket.io": "^4.4.1",
    "v-viewer": "^3.0.10",
    "vee-validate": "^4.5.7",
    "vue": "3.2.26",
    "vue-i18n": "^9.2.0-beta.26",
    "vue-router": "4.0.12",
    "vue-select": "^4.0.0-beta.3",
    "vue3-barcode-scanner": "^1.0.4",
    "vue3-dropzone": "^0.0.7",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3",
    "yup": "^0.32.11"
  }
}
